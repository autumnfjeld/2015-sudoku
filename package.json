{
  "name": "2015_sudoku",
  "version": "1.0.0beta",
  "description": "Sudoku game: demonstraing modular, performant Javascript code with well-structure, flexible CSS.",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: ToDo: Set up tests\" && exit 1",
